{% extends 'base.html.twig' %}

{% block title %}ReclamationUser index{% endblock %}

{% block body %}
    <h1>ReclamationUser index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Idrep</th>
                <th>Titre</th>
                <th>Texte</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for reclamation_user in reclamation_users %}
            <tr>
                <td>{{ reclamation_user.id }}</td>
                <td>{{ reclamation_user.idrep }}</td>
                <td>{{ reclamation_user.titre }}</td>
                <td>{{ reclamation_user.texte }}</td>
                <td>
                    <a class="btn btn-outline-primary btn-fw"  href="{{ path('app_reclamation_user_show', {'id': reclamation_user.id}) }}">show</a>
                    <a class="btn btn-outline-primary btn-fw" href="{{ path('app_reclamation_user_edit', {'id': reclamation_user.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
